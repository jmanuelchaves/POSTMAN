			"item": [
				{
					"name": "All Workspaces",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Tests for GET https://api.clockify.me/api/v1/workspaces\r",
									"// Robust parsing with error handling\r",
									"let isJson = false;\r",
									"let data = null;\r",
									"\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"  pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response is JSON\", function () {\r",
									"  try {\r",
									"    data = pm.response.json();\r",
									"    isJson = Array.isArray(data) || typeof data === 'object';\r",
									"  } catch (e) {\r",
									"    isJson = false;\r",
									"  }\r",
									"  pm.expect(isJson, \"Response must be valid JSON\").to.be.true;\r",
									"});\r",
									"\r",
									"// If not JSON, short-circuit the remaining tests gracefully\r",
									"if (!isJson) {\r",
									"  // Stop further tests to avoid false failures\r",
									"  pm.test(\"Skipping schema tests due to non-JSON response\", function () {\r",
									"    pm.expect(isJson).to.be.true;\r",
									"  });\r",
									"} else {\r",
									"  // Ensure array and non-empty\r",
									"  pm.test(\"Response is an array with length > 0\", function () {\r",
									"    pm.expect(Array.isArray(data), \"Top-level should be an array\").to.be.true;\r",
									"    pm.expect(data.length, \"Array should not be empty\").to.be.greaterThan(0);\r",
									"  });\r",
									"\r",
									"  // Validate each workspace object\r",
									"  pm.test(\"Each workspace has non-empty id and name\", function () {\r",
									"    if (!Array.isArray(data)) {\r",
									"      pm.expect.fail(\"Top-level response is not an array\");\r",
									"      return;\r",
									"    }\r",
									"    data.forEach((ws, idx) => {\r",
									"      pm.expect(ws, `Item at index ${idx} should be an object`).to.be.an(\"object\");\r",
									"      pm.expect(ws).to.have.property(\"id\");\r",
									"      pm.expect(ws.id, `workspace.id at index ${idx} should be a non-empty string`).to.be.a(\"string\").and.to.have.length.greaterThan(0);\r",
									"      pm.expect(ws).to.have.property(\"name\");\r",
									"      pm.expect(ws.name, `workspace.name at index ${idx} should be a non-empty string`).to.be.a(\"string\").and.to.have.length.greaterThan(0);\r",
									"    });\r",
									"  });\r",
									"\r",
									"  // Validate memberships array and required fields inside each membership\r",
									"  pm.test(\"Each workspace has memberships with userId and membershipStatus\", function () {\r",
									"    if (!Array.isArray(data)) {\r",
									"      pm.expect.fail(\"Top-level response is not an array\");\r",
									"      return;\r",
									"    }\r",
									"    data.forEach((ws, idx) => {\r",
									"      pm.expect(ws, `workspace at index ${idx}`).to.have.property(\"memberships\");\r",
									"      pm.expect(ws.memberships, `memberships at index ${idx} should be an array`).to.be.an(\"array\");\r",
									"      ws.memberships.forEach((m, mIdx) => {\r",
									"        pm.expect(m, `membership at ws[${idx}].memberships[${mIdx}]`).to.be.an(\"object\");\r",
									"        pm.expect(m, `membership at ws[${idx}].memberships[${mIdx}] should have userId`).to.have.property(\"userId\");\r",
									"        pm.expect(m.userId, `userId at ws[${idx}].memberships[${mIdx}] should be a non-empty string`).to.be.a(\"string\").and.to.have.length.greaterThan(0);\r",
									"        pm.expect(m, `membership at ws[${idx}].memberships[${mIdx}] should have membershipStatus`).to.have.property(\"membershipStatus\");\r",
									"        pm.expect(m.membershipStatus, `membershipStatus at ws[${idx}].memberships[${mIdx}] should be a non-empty string`).to.be.a(\"string\").and.to.have.length.greaterThan(0);\r",
									"      });\r",
									"    });\r",
									"  });\r",
									"\r",
									"  // Save first workspace id to environment variable\r",
									"  pm.test(\"Save first workspace id to environment variable\", function () {\r",
									"    pm.expect(Array.isArray(data) && data.length > 0, \"Array must be non-empty to save first id\").to.be.true;\r",
									"    const firstId = data[0] && data[0].id;\r",
									"    pm.expect(firstId, \"First workspace should have an id\").to.be.a(\"string\").and.to.have.length.greaterThan(0);\r",
									"    pm.environment.set(\"firstWorkspaceId\", firstId);\r",
									"  });\r",
									"}\r",
									"\r",
									"// Performance test\r",
									"pm.test(\"Response time is below 2000 ms\", function () {\r",
									"  pm.expect(pm.response.responseTime).to.be.below(2000);\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
